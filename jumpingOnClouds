int jumpingOnClouds(vector<int> c) {
    int count=0,i=0;
    while(i < c.size()-1){
        if (i+2==c.size()){
            count++;
            break;
        } 
        
        if(c[i+2]==0 ){
            count++;
            i=i+2;
        }
        else{
            count++;
            i++;
        }
    }
        
    return count;

}
